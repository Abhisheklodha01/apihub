generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Image {
  id          Int    @id @default(autoincrement())
  name        String
  url         String
  description String
}

model Todo {
  id          Int     @id @default(autoincrement())
  name        String
  description String?
}

model Cat {
  id          Int    @id @default(autoincrement())
  name        String
  breed       String
  location    String
  imageUrl    String
  description String
}

model Dog {
  id          Int    @id @default(autoincrement())
  name        String
  breed       String
  location    String
  imageUrl    String
  description String
}

model InstaReels {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  url         String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model YoutubeVideo {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  url         String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Books {
  id          Int      @id @default(autoincrement())
  title       String
  author      String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  imageUrl    String
}

model Movies {
  id          Int      @id @default(autoincrement())
  title       String
  director    String
  poster      String
  realeseDate String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Jokes {
  id          Int      @id @default(autoincrement())
  title       String
  type        String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Quotes {
  id        Int      @id @default(autoincrement())
  author    String
  quote     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Restaurants {
  id             Int      @id @default(autoincrement())
  name           String
  location       String
  city           String
  foodType       String
  priceRange     String
  restuarantType String
  description    String
  createdAt      DateTime @default(now())
  updatedAt      DateTime
}

model FoodNutritions{
  id          Int      @id @default(autoincrement())
  food_name   String
  servingSize String
  calories    Int
  protein     Int
  carbohydrates Int
  fat         Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Hotels {
  id             Int      @id @default(autoincrement())
  name           String
  location       String
  city           String
  rating         Int
  amenities      String
  description    String
  createdAt      DateTime @default(now())
  updatedAt      DateTime
}

model ProgrammingLanguage {
  id          Int      @id @default(autoincrement())
  name        String
  creator     String
  year        Int
  runtime     Int
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}